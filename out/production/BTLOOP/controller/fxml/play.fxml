<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.Pane?>

<?import javafx.scene.Group?>
<Pane fx:id="root" prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/25"
      xmlns:fx="http://javafx.com/fxml/1" fx:controller="controller.control.Play" style="-fx-background-color: black">
    <Group fx:id="buttonGroup">
        <Button layoutX="553.0" mnemonicParsing="false" onAction="#clickPause" text="Pause"/>
        <Button mnemonicParsing="false" onAction="#clickToHome" text="Home"/>
    </Group>
    <Group fx:id="gamePlay"></Group>
</Pane>
